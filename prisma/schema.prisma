generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = "file:./../main.db"
}

model Channel {
  server  String @id
  channel String @unique(map: "sqlite_autoindex_Channel_2")
  Config  Config @relation(fields: [server], references: [server], onDelete: NoAction, onUpdate: NoAction)
}

model Config {
  server      String   @id
  globalEmbed Boolean  @default(true)
  Channel     Channel?
}
